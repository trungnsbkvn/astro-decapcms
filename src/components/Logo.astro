---
import logoIcon from '~/assets/images/logo.png';
import Image from '~/components/common/Image.astro';
---

<Image
  src={logoIcon}
  class="inset-0 object-cover w-full h-full mb-6 rounded shadow-lg bg-gray-400 dark:bg-slate-700"
  height={60}
  alt="YP"
  aspectRatio="16:9"
  loading="lazy"
  decoding="async"
/>
