---
import logoIcon from '~/assets/images/youth and partners/LOGO-YP.png';
import Image from '~/components/common/Image.astro';
---

<Image
  src={logoIcon}
  class="inset-0 object-cover w-full h-full rounded shadow-lg bg-gray-400 dark:bg-slate-700"
  width={107}
  height={60}
  widths={[107]}
  alt="YP"
  layout="fixed"
  loading="lazy"
  decoding="async"
  format="webp"
/>
