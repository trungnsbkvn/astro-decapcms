---
// Google Analytics with Partytown
const GA_ID = 'G-HB9LNKY6F2';
---

<script is:inline async src={`https://www.googletagmanager.com/gtag/js?id=${GA_ID}`} type="text/partytown"></script>
<script is:inline type="text/partytown" define:vars={{ GA_ID }}>
  window.dataLayer = window.dataLayer || [];
  window.gtag = function () {
    dataLayer.push(arguments);
  };
  window.gtag('js', new Date());
  window.gtag('config', GA_ID);
</script>
